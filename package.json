{
  "name": "@imo-tikuwa/vue3-bootstrap5-composables",
  "version": "0.2.2",
  "description": "Vue3&Bootstrap5 composable plugins.",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue3-bootstrap5-composables.umd.cjs",
  "module": "./dist/vue3-bootstrap5-composables.js",
  "exports": {
    ".": {
      "import": "./dist/vue3-bootstrap5-composables.js",
      "require": "./dist/vue3-bootstrap5-composables.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "bootstrap": "^5.3.2",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@popperjs/core": "^2.11.8",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/bootstrap": "^5.2.10",
    "@types/node": "^18.18.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "typescript": "~5.2.0",
    "vite": "^4.4.11",
    "vue-tsc": "^1.8.19"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imo-tikuwa/vue3-bootstrap5-composables.git"
  },
  "homepage": "https://github.com/imo-tikuwa/vue3-bootstrap5-composables"
}
